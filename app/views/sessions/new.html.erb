<h2>Login to your Account</h2><br>

<%= render partial: "layouts/errors", locals: {object: @user} %>

<%= form_for :user do |f| %>
            
<%= f.label :email %>
<%= f.email_field :email %> <br>
<br>

<%= f.label :password %>
<%= f.password_field :password %> <br>
<br>

<%= f.submit "Login" %>

<% end %>